{
    "repo": "sivel/speedtest-cli",

    "description": "Command line interface for testing internet bandwidth",

    "logo": [
        "nwwwwn",
        "wnnnnw",
        "wnnwnw",
        "wnwnnw",
        "wnnnnw",
        "nwnnwn"
    ],

    "dependencies": ["python3"],
    "actions": {
        "install": [
            "cp . /usr/local/lib/speedtest",
            "python3 /usr/local/lib/speedtest/setup.py install"
        ],
        "remove": [
            "sudo rm -r /usr/local/bin/speedtest /usr/local/lib/speedtest"
        ]
    }
}
