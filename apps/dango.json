
{
    "repo": "gsobell/dango",

    "description": "dango is a shell wrapper for the ASCII interface of GNU Go",

    "logo": [
        "yyyyyy",
        "yyywwy",
        "yyyywy",
        "yyyyyy",
        "ydyyyy",
        "yydyyy",
    ],

    "dependencies": ["gnugo"],
    "actions": {
        "install":
            "sudo mkdir /usr/local/bin/dango"
            "sudo chmod +x dango.sh"
            "sudo mv ./dango.sh /usr/local/bin/dango/"
        ],
        "remove": [
            "sudo mv -riv /usr/local/bin/dango /dev/null"
        ]
    }
}
