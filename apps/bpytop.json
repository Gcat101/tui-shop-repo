{
    "repo": "aristocratos/bpytop",

    "description": "Linux/OSX/FreeBSD resource monitor",

    "logo": [
        "yynbnb",
        "ynybnb",
        "ynybbb",
        "yynnbn",
        "ynynbn",
        "yynnbn"
    ],

    "dependencies": ["python3", "python3-pip"],
    "actions": {
        "install": [
            "python3 -m pip install psutil",
            "sudo make install"
        ],
        "remove": [
            "sudo make uninstall"
        ]
    }
}
