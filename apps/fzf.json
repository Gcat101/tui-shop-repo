{
    "repo": "junegunn/fzf",

    "description": "A command-line fuzzy finder",

    "logo": [
        "rrgggg",
        "ggrrgg",
        "ggggrr",
        "ggggrr",
        "ggrrgg",
        "rrgggg"
    ],

    "dependencies": ["golang", "make"],
    "actions": {
        "install": [
            "./install --no-key-bindings --completion --no-update-rc",
            "sudo mv ./bin/* /usr/bin"
        ],
        "remove": [
            "sudo rm /usr/bin/fzf*"
        ]
    }
}
